        <template id="triangle_slider" inherit_id="website.snippets" name="Home Slider">
            <xpath expr="//div[@id='snippet_content']" position="after">
                <div id="snippet_triangle" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-caret-right"/>New: Mega Snippets
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="website_agency.blog_mega_menu"/>
                        <t t-snippet="website_agency.langing_mega_menu"/>
                        <t t-snippet="website_agency.page_mega_menu"/>
                        <t t-snippet="website_agency.demo_mega_menu"/>
<!--                        <t t-snippet="website_agency.docs_mega_menu"/>-->
                    </div>
                </div>
            </xpath>
